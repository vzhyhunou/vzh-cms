{
  "name": "vzh-cms",
  "version": "0.0.1",
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-admin": "^3.6.1",
    "ra-language-russian": "^3.2.1",
    "history": "^4.10.1",
    "query-string": "^6.1.0",
    "recompose": "^0.27.1",
    "lodash": "^4.17.10",
    "classnames": "^2.2.6",
    "jwt-decode": "^2.2.0",
    "recursive-keys": "^0.9.0",
    "js-md5": "^0.7.3",
    "html-react-parser": "^0.10.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "react-scripts": "^3.4.1",
    "@testing-library/react": "^10.0.2",
    "jest-environment-jsdom-sixteen": "^1.0.3"
  },
  "scripts": {
    "start": "CI=true PORT=3010 react-scripts start",
    "build": "react-scripts build && mkdir -p ../target/classes && mv build ../target/classes/static",
    "test": "CI=true react-scripts test --env=jest-environment-jsdom-sixteen",
    "transpile": "rm -rf dist && NODE_ENV=production babel src -d dist --copy-files && rm -r dist/__mocks__ && find dist -name *.test.js -delete -o -name setupTests.js -delete && echo '{\"name\":\"vzh-cms\",\"version\":\"0.0.1\"}' > dist/package.json"
  },
  "proxy": "http://localhost:8090",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
